{
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "https://nest-futura.vercel.app/api/:path*"
      },
      {
        "source": "/(.*)",
        "destination": "/server/index.server.html"
      }
    ],
    "functions": {
      "api/**/*.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }